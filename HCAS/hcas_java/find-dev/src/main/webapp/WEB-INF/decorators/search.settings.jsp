<div id="settings" class="tabpage">
	<div id="settingsContent">
		<h4>Characters:</h4>
		<p class="input-prepend input-append">
			<button id="decChars" class="btn">
				<i class="icon icon-minus"></i>
			</button>
			<input id="chars" type="text" class="span1" value="200">
			<button id="incChars" class="btn">
				<i class="icon icon-plus"></i>
			</button>
		</p>

        <h4>Min Score:</h4>
        <p class="input-prepend input-append">
            <button id="decMinScore" class="btn">
                <i class="icon icon-minus"></i>
            </button>
            <input id="minScore" type="text" class="span1" value="0">
            <button id="incMinScore" class="btn">
                <i class="icon icon-plus"></i>
            </button>
        </p>

		<h4>Content:</h4>
		<div id="resultsDbs"></div>
		<p></p>

		<h4>Suggestions:</h4>
		<div id="suggestionsDbs"></div>
	</div>
</div>