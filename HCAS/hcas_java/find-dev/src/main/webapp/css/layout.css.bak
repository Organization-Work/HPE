.results {
	display: none;
}

[class^="big-icon-"],[class*=" big-icon-"] {
	background-position: 20px 26px;
	display: inline-block;
	height: 26px;
	line-height: 26px;
	vertical-align: text-top;
	width: 20px;
}

.big-icon-fire {
	background-image: url(../img/glyphicons_022_fire.png);
}

.big-icon-exclamation-sign {
	background-image: url(../img/glyphicons_196_circle_exclamation_mark.png);
	width: 26px;
	background-position: 26px 26px;
}

.big-icon-wifi {
	background-image: url(../img/glyphicons_032_wifi_alt.png);
	width: 26px;
	background-position: 26px 26px;
}

.navbar .nav li p {
	padding: 10px 5px 0;
}

.notify.btn {
	pointer-events: none;
}

.btn .tooltip-arrow {
	top: 50%;
	border-style: solid;
	margin-top: -8px;
	border-width: 8px 8px 8px 0;
	right: 100%;
	display: block;
	border-color: transparent;
	border-right-color: #DA4F49;
}

.resultsList .squishable {
    margin-bottom: 30px;
}
.squishable {
	overflow: hidden;
	opacity: 1;
	max-height: 1000px;
	-webkit-transition: opacity 0.25s ease, max-height 0.25s
		cubic-bezier(1, 0, 1, 0);
	-moz-transition: opacity 0.25s ease, max-height 0.25s
		cubic-bezier(1, 0, 1, 0);
	-o-transition: opacity 0.25s ease, max-height 0.25s
		cubic-bezier(1, 0, 1, 0);
	transition: opacity 0.25s ease, max-height 0.25s
		cubic-bezier(1, 0, 1, 0);
}

.squishable.squish {
	opacity: 0;
	max-height: 0px;
	-webkit-transition: opacity 0.25s ease, max-height 0.5s
		cubic-bezier(0, 1, 0, 1);
	-moz-transition: opacity 0.25s ease, max-height 0.5s
		cubic-bezier(0, 1, 0, 1);
	-o-transition: opacity 0.25s ease, max-height 0.5s
		cubic-bezier(0, 1, 0, 1);
	transition: opacity 0.25s ease, max-height 0.5s cubic-bezier(0, 1, 0, 1);
    margin-bottom: 0;
}

.row-fluid .span4.slim {
	margin-left: 0;
	padding-left: 0;
}

#topNews {
	padding-bottom: 30px;
}

