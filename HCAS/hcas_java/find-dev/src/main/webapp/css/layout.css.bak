}

.hiddenSummary {
	-webkit-transition: opacity 0.5s ease, max-height 0.5s ease;
	-moz-transition: opacity 0.5s ease, max-height 0.5s ease;
	-o-transition: opacity 0.5s ease, max-height 0.5s ease;
	transition: opacity 0.5s ease, max-height 0.5s ease;
	opacity: 0;
	max-height: 0;
	overflow: hidden;
	margin-left: 25px;
}

.hiddenSummary.showHiddenSummary {
	-webkit-transition: opacity 0.5s ease 0.1s, max-height 1.5s ease;
	-moz-transition: opacity 0.5s ease 0.1s, max-height 1.5s ease;
	-o-transition: opacity 0.5s ease 0.1s, max-height 1.5s ease;
	transition: opacity 0.5s ease 0.1s, max-height 1.5s ease;
	opacity: 1;
	max-height: 500px;
}

