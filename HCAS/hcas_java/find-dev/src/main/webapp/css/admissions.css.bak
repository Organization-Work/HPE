head, body {
    height:100%;
    /*overflow-y: hidden;*/
}

.full-height {
    position: absolute;
    top: 70px;
    bottom: 0;
    overflow-y: auto;
}

#restricted {
    text-align: center;
    width: 100%;
    /*height: 100%;*/
}

#restricted-text {
    position: absolute;
    top: 45%;
    left: 0;
    right: 0;
}

#record_container {
    left: 20px;
    right: 30%;
}

#record_container.has-admissions {
    left: 10%;
}



#docview_rightpanel {
    right: 0;
    left: 70%;
    padding: 10px 10px 20px 10px;
	height:auto;
	display:block;
	position: absolute;
	border-width:3px;
	border-color:darkgray;
}
#agentstore-fields {
	position:relative;
	width:90%;
	height:auto;
 	display:block;
}
#filters-viewer {
	position:relative;
	width:90%;
	height:auto;
 	display:block;
}

