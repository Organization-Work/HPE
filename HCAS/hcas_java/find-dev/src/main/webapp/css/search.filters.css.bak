fieldset.filter-group {
    border: 1px groove #ddd !important;
    padding: 0 5px 5px 5px !important;
    margin: 0 0 2px 0 !important;
    -webkit-box-shadow:  0px 0px 0px 0px #000;
            box-shadow:  0px 0px 0px 0px #000;
}

legend.filter-group {
    width: inherit; /* Or auto */
    padding: 0 10px; /* To give a bit of padding on the left and right */
    border-bottom: none;
    margin-bottom: 0;

}

legend.filter-group select {
    font-size: 12px;
    height: 20px;
    padding: 1px 0px 0px 0px;
    line-height: 24px;
    width: auto;
}

.filter-item select.field-operator {
    font-size: 12px;
    height: 20px;
    padding: 1px 0px 0px 0px;
    line-height: 24px;
    width: auto;
}


legend.filter-group input[type="text"] {
    font-size: 12px;
    height: 20px;
    padding: 2px 0px 0px 0px;
    line-height: 24px;
    width: 100px;
}

legend.filter-group .dropdown {
    display: inline-block;
}

fieldset.filter-group.cohortFilterGroup {
    border: 1px dashed black !important;
}

ul.group-cohort a,
ul.group-close a {
    font-size: 12px;
}

.icon-cohort {
    vertical-align: middle;
}

.dropdown-menu .cohort-options,
.dropdown-menu .grpclose-options {
    padding-right: 10px;
    padding-left: 5px;
}

.cohort-options:before,
.grpclose-options:before {
    content: "\3000";
}

.cohort-options.selected:before,
.grpclose-options.deactivated:before {
    content: "\2713";
}

fieldset.deactivated {
    background-color: #f3f3f3;
    filter: contrast(70%);
    -webkit-filter: contrast(70%);    
}

.result-view {
    margin-right: 25px;
    color: black;
}

.result-view select {
    width: auto;
    height: 22px;
    line-height: 22px;
    padding: 2px;
    vertical-align: inherit;
}

.search-mode select {
    width: auto;
    height: 22px;
    line-height: 22px;
    padding: 1px;
    font-size: 12px;
    vertical-align: middle;
}

.search-mode h2 {
    display: inline;
}

#searchGo {
    margin-left: 0px;
}

#searchArea {
    width: 100%;
} 

#filterSyncError {
    margin-top: 10px;
    color: red;
    font-size: 12px;
}

.view-results-label {
    vertical-align: top;
    line-height: 28px;
}

.view-results-label.error,
.view-results-label.loading {
    text-decoration: line-through;
    color: red;
}

.view-results-label.loading:before {
    content: url('../img_logos/blue-loading.gif');
}


@media (min-width: 1500px) {
    #searchArea {
        width: 220px;
    } 
}

